{
    "name": "sofyco/spider-bundle",
    "type": "symfony-bundle",
    "version": "3.0.2",
    "description": "Symfony Spider Bundle",
    "keywords": ["php", "symfony", "crawler", "scraper", "parser"],
    "homepage": "https://github.com/sofyco/spider-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofiia Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.5",
        "ext-simplexml": "*",
        "sofyco/spider": "^3.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/messenger": "^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\SpiderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\SpiderBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
